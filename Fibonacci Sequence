def nonNeg(n):  # checks whether a number is negative
    if n > 0:
        return True
    else:
        return False

def fib(n):    # checks digit N of the Fibonacci Sequence
    if nonNeg(n):
        if n == 1:
            return 1
        elif n == 2:
            return 1
        else:
            return fib(n - 1) + fib(n - 2)

def listoffib(n): # prints Fibonacci Sequence to digit N
    for i in range(0,n):
        print fib(i)
